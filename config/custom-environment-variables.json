{
  "node-env": "NODE_ENV",
  "port": "DARTS_PORTAL_PORT",
  "hostname": "DARTS_PORTAL_URL",
  "darts-api": {
    "url": "DARTS_API_URL"
  },
  "authentication": {
    "azureAdB2cOriginHost": "DARTS_AZUREAD_B2C_ORIGIN_HOST",
    "allowAuthBootstrap": "DARTS_ALLOW_AUTH_BOOTSTRAP"
  },
  "session": {
    "useSecureCookies": "DARTS_SESSION_USE_SECURE_COOKIES",
    "prefix": "DARTS_SESSION_PREFIX",
    "ttlInSeconds": "DARTS_SESSION_TTL"
  }
}
